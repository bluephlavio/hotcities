{
  "name": "hotcities",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js --fix",
    "cls": "rm -rf dist/",
    "start:server": "node ./dist/server/server.bundle.js",
    "start:server:dev": "cross-env NODE_ENV=development nodemon",
    "watch:server:dev": "cross-env NODE_ENV=development webpack --config webpack.config.server.js --watch",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.server.js && npm-run-all --parallel watch:server:dev start:server:dev",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.client.js",
    "build": "yarn run build:server && yarn run build:client"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
