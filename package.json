    "dateformat": "^3.0.3",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "express-handlebars": "^3.0.0",
    "express-jwt": "^5.3.1",
    "flickr-sdk": "^3.7.0",
    "font-awesome5": "^1.0.5",
